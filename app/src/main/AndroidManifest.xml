<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.reachthegoal">
<uses-permission android:name="android.permission.FOREGROUND_SERVICE"/>
    <application
        android:name=".NotificationChannel"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".CreateGoal"/>
        <activity android:name=".Calendar"/>
        <activity android:name=".GoalActivity"/>
        <activity android:name=".NotificationActivity"/>
        <activity android:name=".BrakeActivity"/>
        <activity android:name=".GoalSettingsActivity"/>
        <activity android:name=".CheckListActivity"/>
        <receiver android:name=".AlarmReceiverActivity"/>
    </application>

</manifest>